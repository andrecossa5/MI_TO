singularity { 
    enabled = true   
    runOptions = ' --bind /hpcnfs/ '
}

process {   
    container = '/hpcnfs/scratch/PGP/MI_TO_benchmark/MI_TO/images/mito_benchmark.sif'   
    executor = 'pbspro'   
    queue = '-q nocg_workq'   
}

env {   
    PATH = '$PATH:/hpcnfs/software/singularity/3.7.0/bin/:/hpcnfs/software/anaconda/anaconda3/bin/'  
}                                                                                                                                                                                                                  

